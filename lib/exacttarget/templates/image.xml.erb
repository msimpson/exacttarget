<system>

  <system_name>filemanagement</system_name>
  <action>filemove</action>
  <source>
    <location><%= @ftp[:location] %></location>
    <files>
      <% @files.each do |file| %>
        <filename><%= file %></filename>
      <% end %>
    </files>
  </source>
  <destination>imageimport</destination>
  
</system>
